{
  "name": "garvinhicking/tdk-cli",
  "description": "TYPO3 Composer Development Kit: Command Line Interface Tools",
  "type": "project",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "Jochen Roth",
      "email": "jochen.roth@b13.com"
    },
    {
      "name": "Garvin Hicking",
      "email": "blog@garv.in"
    }
  ],
  "autoload-dev": {
    "psr-4": {
      "GarvinHicking\\TdkCli\\": "Scripts/"
    }
  },
  "scripts": {
    "tdk:clone": "GarvinHicking\\TdkCli\\Cli::cloneRepository",
    "tdk:what": "GarvinHicking\\TdkCli\\Cli::help"
  },
  "require": {
    "garvinhicking/tdk-core": "@dev",
    "php": "^8.1"
  },
  "require-dev": {
    "composer/composer": "^2.6"
  },
  "config": {
    "allow-plugins": {
      "typo3/class-alias-loader": true,
      "typo3/cms-composer-installers": true,
      "garvinhicking/tdk-core": true
    }
  }
}
